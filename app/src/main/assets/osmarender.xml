<?xml version="1.0" encoding="UTF-8"?>
<rendertheme xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" map-background="#e5e3e0"
    version="1" xmlns="http://opensciencemap.org/rendertheme"
    xsi:schemaLocation="http://opensciencemap.org/rendertheme https://raw.githubusercontent.com/mapsforge/vtm/master/resources/rendertheme.xsd">

    <!--###### TEXT styles ######-->

    <!--default label-->
    <style-text caption="true" dy="10" fill="#2d51bc" font-family="medium" id="caption-small-blue"
        k="name" size="11" stroke="#ffffff" stroke-width="2.0" style="italic" />
    <!--education-->
    <style-text caption="true" dy="20" fill="#0a4f0a" font-family="medium" id="education" k="name"
        size="11" stroke="#ffffff" stroke-width="2.0" />
    <!--health-->
    <style-text caption="true" dy="20" fill="#da0092" font-family="medium" id="health" k="name"
        size="11" stroke="#ffffff" stroke-width="2.0" style="bold" />
    <!--roads-->
    <style-text style="bold" id="highway-road" k="name" size="13" stroke="#d0d0d0"
        stroke-width="2.0" />
    <style-text style="bold" id="highway-track" k="name" size="13" stroke="#fff2de"
        stroke-width="2.0" />
    <!--references-->
    <style-text style="bold" fill="#606060" id="ref" k="ref" priority="2" size="12" stroke="#ffffff"
        stroke-width="2.0" />
    <style-text caption="true" style="bold" fill="#ffffff" id="ref-caption" k="ref" priority="2"
        size="12" bg-fill="#007f00" />


    <!--###### AREA styles ######-->

    <!--water-->
    <style-area fill="#b5d6f1" id="water" />


    <!--###### LINE styles ######-->

    <style-line id="trunk" stroke="#f9b29c" width="1.2" />
    <style-line id="motorway" stroke="#FFD966" width="1.5" />
    <style-line id="primary" stroke="#fcd6a4" width="1.3" />
    <style-line id="secondary" stroke="#f7fabf" width="1.1" />
    <style-line id="tertiary" stroke="#FFFFFF" width="1.0" />

    <!--###### SYMBOL styles ######-->

    <!-- oneway -->
    <style-symbol id="oneway" repeat="true" src="assets:jednosmerna.svg" />


    <!--###### ASSIGNMENT ######-->

    <m e="way" k="natural" v="issea|sea">
        <area mesh="true" use="water" />
    </m>

    <m e="way" k="natural" v="nosea">
        <area fill="#f8f8f8" mesh="true" />
    </m>

    <!-- ways -->
    <m e="way">
        <!-- landuse -->
        <m k="landuse">
            <m select="first">
                <m v="military">
                    <area fill="#40eeedea" />
                </m>
                <m v="residential|farmyard">
                    <area fill="#e4e4e4" />
                </m>
                <m v="retail">
                    <area fill="#ffebeb" />
                </m>
                <m v="industrial|brownfield|railway">
                    <area fill="#e7cbb2" />
                </m>
                <m v="commercial">
                    <area fill="#ffffc0" />
                </m>
                <m v="construction|greenfield">
                    <area fill="#a47c41" />
                </m>
                <m v="garages">
                    <area fill="#d6d6e4" />
                </m>

                <m v="landfill|quarry">
                    <area fill="#e9dd72" />
                </m>
                <m v="cemetery">
                    <area fill="#c7f1a3" />
                </m>
                <m v="field|farm|farmland|meadow|grass">
                    <area fill="#ebfff2" />
                </m>
                <m v="allotments|village_green|recreation_ground">
                    <area fill="#bde3cb" />
                </m>
                <m v="reservoir|basin">
                    <area fill="#b5d6f1" />
                </m>
            </m>
            <m k="name" zoom-min="16">
                <text use="caption-small-blue" />
            </m>
        </m>


        <m k="natural" v="grassland|scrub">
            <area fill="#6fc18e" />
        </m>

        <m k="natural|landuse" v="forest|wood">
            <area fill="#6fc18e" />

            <m zoom-min="16">
                <text use="caption-small-blue" />
            </m>
        </m>


        <!-- amenity -->
        <m k="amenity">
            <m select="first">
                <m v="kindergarten|school|college|university">
                    <area fill="#cdabde" />
                </m>
                <m v="grave_yard">
                    <area fill="#c7f1a3" />
                </m>
                <m v="parking">
                    <area fill="#ffffc0" />
                    <m k="access" v="private" zoom-min="15">
                        <area fill="#40ff0000" />
                    </m>
                </m>
                <m closed="yes" v="fountain">
                    <area fill="#b5d6f1" />
                </m>
            </m>
        </m>


        <!-- natural -->
        <m k="natural">
            <m select="first">
                <m v="glacier">
                    <area fill="#fafaff" />
                </m>
                <m v="land">
                    <area fill="#f8f8f8" />
                </m>
                <m v="beach">
                    <area fill="#eecc55" />
                </m>
                <m v="heath">
                    <area fill="#ffffc0" />
                </m>
                <m v="marsh|wetland">
                    <area fill="#deecb9" />
                </m>
            </m>
            <m k="name" zoom-min="16">
                <text use="caption-small-blue" />
            </m>
        </m>


        <!-- leisure -->
        <m k="leisure">
            <m select="first">
                <m v="park|garden|golf_course|common|green">
                    <area fill="#c7f1a3" />
                </m>
                <m v="nature_reserve">
                    <area fill="#40abe29c" />
                </m>

                <m v="stadium|sports_centre|water_park">
                    <area fill="#bde3cb" />
                </m>
                <m v="track">
                    <m k="area" v="yes|true">
                        <area fill="#bde3cb" />
                    </m>
                    <m k="area" v="~|no|false">
                        <line stroke="#6fc18e" width="0.75" />
                        <line stroke="#bde3cb" width="0.7" />
                    </m>
                </m>
                <m v="swimming_pool">
                    <area fill="#b5d6f1" />
                </m>
            </m>
            <m k="name" zoom-min="16">
                <text use="caption-small-blue" />
            </m>
        </m>


        <!-- waterways -->
        <m k="waterway">
            <!-- waterway casings -->

            <m v="canal">
                <line stroke="#b0b0b0" width="1.0" />
            </m>

            <!-- waterway cores -->
            <m v="ditch">
                <line stroke="#b5d6f1" width="0.1" />
            </m>
            <m v="drain">
                <line fade="13" fix="true" stroke="#b5d6f1" width="1.2" />
            </m>
            <m v="canal">
                <line stroke="#b5d6f1" width="0.8" />
            </m>
            <m v="stream">
                <line stroke="#b5d6f1" width="0.275" />
            </m>
            <m v="river" zoom-min="12">
                <line stroke="#a5b6e1" width="1.5" />
            </m>
            <m v="river" zoom-max="11">
                <line fix="true" stroke="#a5b6e1" width="1.5" />
            </m>

            <m v="dock">
                <area fill="#b5d6f1" />
            </m>
            <m v="riverbank">
                <area fill="#b5d6f1" />
            </m>
            <m v="weir">
                <line stroke="#000044" width="0.375" />
            </m>
            <m v="dam">
                <line stroke="#000000" width="0.5" />
            </m>
            <m k="lock" v="yes|true">
                <line cap="butt" stroke="#000000" width="2.0" />
                <line cap="butt" stroke="#f8f8f8" width="1.5" />
            </m>

            <text fill="#404000" k="name" size="13" stroke="#aaffffff" stroke-width="2.0" />
        </m>

        <!-- tourism areas -->
        <m k="tourism">
            <m v="attraction">
                <area fill="#f2caea" />
            </m>
            <m v="zoo|picnic_site|caravan_site|camp_site">
                <area fill="#c7f1a3" />
            </m>
        </m>


        <m k="natural" v="water">
            <area use="water" />
            <caption area-size="0.2" fill="#404000" k="name" size="13" stroke="#aaffffff"
                stroke-width="2.0" />
        </m>

        <!--<m k="natural" v="coastline" closed="no">
          <line stroke="#708599" fix="true" cap="butt" width="2.0" />
        </m>-->

        <!-- tunnel -->
        <m k="tunnel" v="true|yes">
            <!-- highway tunnels -->
            <m k="highway">
                <!-- highway tunnel casings -->
                <m v="steps|footway|path">
                    <line cap="butt" dasharray="5,5" stroke="#707070" width="0.6" />
                </m>
                <m v="track|cycleway|bridleway|service">
                    <line cap="butt" dasharray="5,5" stroke="#707070" width="1.0" />
                </m>
                <m v="construction">
                    <line cap="butt" dasharray="5,5" stroke="#707070" width="1.1" />
                </m>
                <m v="road">
                    <line cap="butt" dasharray="5,5" stroke="#707070" width="1.3" />
                </m>
                <m v="pedestrian|unclassified|residential|living_street|byway">
                    <line cap="butt" dasharray="5,5" stroke="#707070" width="3.0" />
                </m>
                <m v="tertiary|secondary_link|primary_link|trunk_link|motorway_link|secondary|primary|trunk">
                    <line cap="butt" dasharray="5,5" stroke="#707070" width="3.0" />
                </m>
                <m v="motorway">
                    <line cap="butt" dasharray="5,5" stroke="#707070" width="2.0" />
                </m>

                <!-- highway tunnel cores -->
                <m v="steps|footway|path">
                    <line cap="butt" dasharray="5,5" stroke="#ffffff" width="0.45" />
                </m>
                <m v="track">
                    <line cap="butt" dasharray="5,5" stroke="#ffffff" width="0.7" />
                </m>
                <m v="cycleway|bridleway|service">
                    <line cap="butt" dasharray="5,5" stroke="#ffffff" width="0.85" />
                </m>
                <m v="construction">
                    <line cap="butt" dasharray="5,5" stroke="#ffffff" width="1.0" />
                </m>
                <m v="road">
                    <line cap="butt" dasharray="5,5" stroke="#ffffff" width="1.25" />
                </m>
                <m v="pedestrian|unclassified|residential|living_street|byway">
                    <line cap="butt" dasharray="5,5" stroke="#ffffff" width="1.35" />
                </m>
                <m v="tertiary|secondary_link|primary_link|trunk_link|motorway_link|secondary|primary|trunk">
                    <line cap="butt" dasharray="5,5" stroke="#ffffff" width="1.5" />
                </m>
                <m v="motorway">
                    <line cap="butt" dasharray="5,5" stroke="#ffffff" width="1.7" />
                </m>
            </m>

            <!-- railway tunnel -->
            <m k="railway">
                <m v="tram|subway|light_rail|narrow_gauge">
                    <line cap="butt" dasharray="4,4" stroke="#880f0f4c" width="0.25" />
                </m>
                <m v="rail">
                    <line cap="butt" dasharray="4,4" stroke="#aa333333" width="0.4" />
                </m>
            </m>
        </m>


        <!-- highway area -->
        <m k="area" v="yes|true">
            <m k="highway">
                <m v="footway|footpath">
                    <area fill="#e5e0c2" />
                </m>
                <m v="pedestrian">
                    <area fill="#e5e0c2" />
                </m>
                <m v="path">
                    <area fill="#d0d0d0" />
                </m>
                <m v="service">
                    <area fill="#ffffff" />
                </m>
                <m v="unclassified">
                    <area fill="#ffffff" />
                </m>
                <m v="residential">
                    <area fill="#ffffff" />
                </m>
                <m v="road">
                    <area fill="#d0d0d0" />
                </m>
                <m v="living_street">
                    <area fill="#ffffff" />
                </m>
            </m>
        </m>

        <!-- platform cores -->
        <m k="highway|railway|public_transport" v="platform">
            <m closed="yes">
                <area fill="#9b9b79" />
            </m>
            <m closed="no">
                <line stroke="#9b9b79" width="0.3" />
            </m>
        </m>


        <!-- runways casings and areas -->
        <m k="aeroway">
            <m closed="yes" v="aerodrome">
                <m zoom-min="12" zoom-max="15">
                    <symbol src="assets:aerodrom.svg" />
                    <caption area-size="0.1" dy="25" fill="#000000" k="name" priority="0" size="13"
                        stroke="#ffffff" stroke-width="2.0" />
                </m>
            </m>
            <m v="apron">
                <area fill="#f0f0f0" />
            </m>
            <m v="terminal">
                <area fill="#f3d6b6" />
                <line cap="butt" fix="true" stroke="#6a5a8e" width="1.0" />
            </m>
        </m>
    </m>

    <m e="way">
        <m k="tunnel" v="~|no|false">
            <m k="area" v="~|no|false">
                <m k="highway">
                    <m k="bridge" v="~|no|false">
                        <m v="steps">
                            <line stroke="#707070" width="0.6" />
                        </m>
                        <m v="footway|path">
                            <line cap="butt" fix="true" stipple="2" stipple-stroke="#909090"
                                stipple-width="1.0" stroke="#00ffffff" width="2.6" />
                        </m>
                    </m>
                </m>
            </m>
        </m>


        <!-- building -->
        <m k="building|building:part">
            <m select="first">
                <m closed="yes">
                    <area fill="#f3d6b6" />
                    <line cap="butt" fix="true" stroke="#6a5a8e" width="1.0" />
                </m>
                <m closed="no">
                    <line cap="butt" fix="true" stroke="#6a5a8e" width="1.0" />
                </m>
            </m>

            <m zoom-min="17">
                <extrusion hsv-s="0.2" hsv-v="1.4" line-color="#506a5a8e" side-color="#eaf3d6b6"
                    top-color="#eaf8d9b9" />
            </m>
        </m>

        <outline-layer cap="round" id="casing" stroke="#707070" width="0.15" />
        <outline-layer cap="butt" id="casing-bridge" stroke="#404040" width="0.25" />

        <m k="highway">
            <m k="tunnel" v="~|no|false">
                <m k="area" select="first" v="~|no|false">

                    <m v="steps">
                        <line cap="butt" dasharray="3,1" stroke="#e5e0c2" width="0.45" />
                        <m zoom-min="15">
                            <text style="bold" k="name" size="13" stroke="#e5e0c2"
                                stroke-width="2.0" />
                        </m>
                    </m>
                    <m v="footway|platform|corridor">
                        <line cap="butt" fix="true" stroke="#e5e0c2" width="1.4" />
                        <m zoom-min="15">
                            <text style="bold" k="name" size="13" stroke="#e5e0c2"
                                stroke-width="2.0" />
                        </m>
                    </m>
                    <m v="cycleway">
                        <line fix="true" stipple="4" stipple-stroke="#0000dd" stipple-width="0.8"
                            stroke="#d1fad1" width="1.4" />
                        <m zoom-min="15">
                            <text style="bold" k="name" size="13" stroke="#d1fad1"
                                stroke-width="2.0" />
                        </m>
                    </m>
                    <m v="bridleway">
                        <line stroke="#d3cb98" width="0.85" />
                        <m zoom-min="15">
                            <text style="bold" k="name" size="13" stroke="#d3cb98"
                                stroke-width="2.0" />
                        </m>
                    </m>
                    <m v="path">
                        <line fix="true" stroke="#d0d0d0" width="1.45" />
                        <m zoom-min="15">
                            <text use="highway-road" />
                        </m>
                    </m>
                    <m select="first" v="track">
                        <m k="tracktype" v="grade1">
                            <line stroke="#fffaf2" width="0.75" />
                            <m zoom-min="15">
                                <text style="bold" k="name" size="13" stroke="#fffaf2"
                                    stroke-width="2.0" />
                            </m>
                        </m>
                        <m k="tracktype" v="grade2">
                            <line stroke="#fff2de" width="0.75" />
                            <m zoom-min="15">
                                <text use="highway-track" />
                            </m>
                        </m>
                        <m k="tracktype" v="grade3">
                            <line stroke="#fff2de" width="0.7" />
                            <m zoom-min="15">
                                <text use="highway-track" />
                            </m>
                        </m>
                        <m k="tracktype" v="grade4">
                            <line stroke="#fff2de" width="0.7" />
                            <m zoom-min="15">
                                <text use="highway-track" />
                            </m>
                        </m>
                        <m k="tracktype" v="grade5">
                            <line stroke="#fff2de" width="0.7" />
                            <m zoom-min="15">
                                <text use="highway-track" />
                            </m>
                        </m>
                        <m k="tracktype" v="~|no|false">
                            <line stroke="#fff2de" width="0.75" />
                            <m zoom-min="15">
                                <text use="highway-track" />
                            </m>
                        </m>
                    </m>

                    <m k="bridge" v="~|no|false">
                        <m select="first">
                            <m v="service">
                                <line stroke="#ffffff" width="0.85" />
                                <m zoom-min="18">
                                    <text style="bold" k="name" size="13" stroke="#ffffff"
                                        stroke-width="2.0" />
                                </m>
                            </m>
                            <m v="construction">
                                <line stroke="#d0d0d0" width="1.0" />
                                <m zoom-min="19">
                                    <text use="highway-road" />
                                </m>
                            </m>
                            <m v="road">
                                <line stroke="#d0d0d0" width="1.25" />
                                <m zoom-min="15">
                                    <text use="highway-road" />
                                </m>
                            </m>
                            <m v="pedestrian">
                                <line stroke="#e5e0c2" width="0.85" />
                                <m zoom-min="18">
                                    <text style="bold" k="name" size="13" stroke="#ffffff"
                                        stroke-width="2.0" />
                                </m>
                            </m>
                            <m v="unclassified|residential|living_street">
                                <line stroke="#ffffff" width="1.25" />
                                <m zoom-min="18">
                                    <text style="bold" k="name" size="13" stroke="#ffffff"
                                        stroke-width="2.0" />
                                </m>
                            </m>
                            <m v="byway">
                                <line stroke="#efadaa" width="1.15" />
                                <m zoom-min="15">
                                    <text style="bold" k="name" size="13" stroke="#ffffff"
                                        stroke-width="2.0" />
                                </m>
                            </m>
                            <m v="tertiary|tertiary_link">
                                <line use="tertiary" />
                                <m zoom-min="14">
                                    <text style="bold" k="name" size="13" stroke="#ffffff"
                                        stroke-width="2.0" />
                                </m>
                            </m>
                            <m v="secondary_link">
                                <line use="secondary" width="-0.1" />
                                <m zoom-min="14">
                                    <text style="bold" k="name" size="13" stroke="#ffffff"
                                        stroke-width="2.0" />
                                </m>
                            </m>
                            <m v="primary_link">
                                <line use="primary" width="-0.1" />
                                <m zoom-min="16">
                                    <text style="bold" stroke="#ffffff" k="name" size="13" />
                                </m>
                            </m>
                            <m v="trunk_link">
                                <line use="trunk" />
                                <m zoom-min="14">
                                    <text style="bold" k="name" size="13" stroke="#ffffff"
                                        stroke-width="2.0" />
                                </m>
                            </m>
                            <m v="motorway_link">
                                <line use="motorway" width="-0.2" />
                                <m zoom-min="14">
                                    <text style="bold" k="name" size="13" stroke="#ffffff"
                                        stroke-width="2.0" />
                                </m>
                            </m>
                            <m v="secondary">
                                <line use="secondary" />
                                <m zoom-min="14">
                                    <text style="bold" k="name" size="13" stroke="#ffffff"
                                        stroke-width="2.0" />
                                </m>
                            </m>
                            <m v="primary">
                                <line use="primary" />
                                <m zoom-min="16">
                                    <text style="bold" fill="#000000" k="name" stroke="#ffffff"
                                        stroke-width="2.0" priority="5" size="15" />
                                </m>
                                <m zoom-max="12">
                                    <text use="ref-caption" />
                                </m>
                                <m zoom-min="13">
                                    <text use="ref" />
                                </m>
                            </m>
                            <m v="trunk">
                                <line use="trunk" />
                                <m zoom-min="17">
                                    <text style="bold" k="name" priority="4" size="14"
                                        stroke="#ffffff" stroke-width="2.0" />
                                </m>
                                <m zoom-max="12">
                                    <text use="ref-caption" />
                                </m>
                                <m zoom-min="13">
                                    <text use="ref" />
                                </m>
                            </m>
                            <m v="motorway">
                                <line use="motorway" />
                                <m zoom-min="14">
                                    <text style="bold" k="name" priority="4" size="13"
                                        stroke="#809bc0" stroke-width="2.0" />
                                </m>
                                <m zoom-max="12">
                                    <text use="ref-caption" />
                                </m>
                                <m zoom-min="13">
                                    <text use="ref" />
                                </m>
                            </m>

                            <!-- add outline for all matches -->
                            <m select="when-matched">
                                <outline use="casing" />
                            </m>
                        </m>
                    </m>

                    <m k="bridge" v="yes|true">
                        <m select="first">
                            <m v="service">
                                <line outline="casing-bridge" stroke="#ffffff" width="0.85" />
                                <m zoom-min="15">
                                    <text style="bold" k="name" size="13" stroke="#ffffff"
                                        stroke-width="2.0" />
                                </m>
                            </m>
                            <m v="construction">
                                <line outline="casing-bridge" stroke="#d0d0d0" width="1.0" />
                                <m zoom-min="15">
                                    <text use="highway-road" />
                                </m>
                            </m>
                            <m v="road">
                                <line outline="casing-bridge" stroke="#d0d0d0" width="1.25" />
                                <m zoom-min="15">
                                    <text use="highway-road" />
                                </m>
                            </m>
                            <m v="pedestrian">
                                <line stroke="#e5e0c2" width="0.85" />
                                <m zoom-min="15">
                                    <text style="bold" k="name" size="13" stroke="#e5e0c2"
                                        stroke-width="2.0" />
                                </m>
                            </m>
                            <m v="unclassified|residential|living_street">
                                <line outline="casing-bridge" stroke="#ffffff" width="1.25" />
                                <m zoom-min="18">
                                    <text style="bold" k="name" size="13" stroke="#ffffff"
                                        stroke-width="2.0" />
                                </m>
                            </m>
                            <m v="byway">
                                <line outline="casing-bridge" stroke="#efadaa" width="1.15" />
                                <m zoom-min="15">
                                    <text style="bold" k="name" size="13" stroke="#efadaa"
                                        stroke-width="2.0" />
                                </m>
                            </m>
                            <m v="tertiary|tertiary_link">
                                <line cap="butt" outline="casing-bridge" use="tertiary" />
                                <m zoom-min="18">
                                    <text style="bold" k="name" size="13" stroke="#ffff90"
                                        stroke-width="2.0" />
                                </m>
                            </m>
                            <m v="secondary_link">
                                <line cap="butt" outline="casing-bridge" use="secondary"
                                    width="-0.1" />
                                <m zoom-min="18">
                                    <text style="bold" k="name" size="13" stroke="#fdbf6f"
                                        stroke-width="2.0" />
                                </m>
                            </m>
                            <m v="primary_link">
                                <line cap="butt" outline="casing-bridge" use="primary"
                                    width="-0.1" />
                                <m zoom-min="18">
                                    <text style="bold" k="name" size="13" />
                                </m>
                            </m>
                            <m v="trunk_link">
                                <line cap="butt" outline="casing-bridge" use="trunk" />
                                <m zoom-min="18">
                                    <text style="bold" k="name" size="13" stroke="#7fc97f"
                                        stroke-width="2.0" />
                                </m>
                            </m>
                            <m v="motorway_link">
                                <line cap="butt" outline="casing-bridge" use="motorway"
                                    width="-0.2" />
                                <m zoom-min="18">
                                    <text style="bold" k="name" size="13" stroke="#809bc0"
                                        stroke-width="2.0" />
                                </m>
                            </m>
                            <m v="secondary">
                                <line cap="butt" outline="casing-bridge" use="secondary" />
                                <m zoom-min="14">
                                    <text style="bold" k="name" size="13" stroke="#fdbf6f"
                                        stroke-width="2.0" />
                                </m>
                            </m>
                            <m v="primary">
                                <line cap="butt" outline="casing-bridge" use="primary" />
                                <m zoom-min="18">
                                    <text style="bold" k="name" stroke="#ffffff" priority="1" size="13" />
                                </m>
                                <m zoom-max="12">
                                    <text use="ref-caption" />
                                </m>
                                <m zoom-min="13">
                                    <text use="ref" />
                                </m>
                            </m>
                            <m v="trunk">
                                <line cap="butt" outline="casing-bridge" use="trunk" />
                                <m zoom-min="18">
                                    <text style="bold" k="name" priority="1" size="13"
                                        stroke="#ffffff" stroke-width="2.0" />
                                </m>
                                <m zoom-max="12">
                                    <text use="ref-caption" />
                                </m>
                                <m zoom-min="13">
                                    <text use="ref" />
                                </m>
                            </m>
                            <m v="motorway">
                                <line cap="butt" outline="casing-bridge" use="motorway" />
                                <m zoom-min="18">
                                    <text style="bold" k="name" priority="1" size="13"
                                        stroke="#ffffff" stroke-width="2.0" />
                                </m>
                                <m zoom-max="12">
                                    <text use="ref-caption" />
                                </m>
                                <m zoom-min="13">
                                    <text use="ref" />
