                            </m>
                        </m>
                    </m>
                </m>
            </m>
        </m>

        <!-- runways cores -->
        <m k="aeroway">
            <m v="runway">
                <line cap="square" stroke="#d4dcbd" width="4.5" />
                <outline use="casing" />
            </m>
            <m v="taxiway">
                <line cap="butt" stroke="#d4dcbd" width="1.8" />
                <outline use="casing" />
            </m>
        </m>

        <!-- highway one-way markers -->
        <m k="tunnel" v="~|false|no">
            <m k="area" v="~|false|no">
                <m k="highway">
                    <m k="oneway" v="yes|true" zoom-min="16">
                        <symbol use="oneway" />
                    </m>
                </m>
            </m>
        </m>

        <m k="aerialway">
            <line cap="butt" stroke="#202020" width="0.4" />
            <line cap="butt" dasharray="2,200" stroke="#202020" width="4.0" />
            <m zoom-min="14">
                <text style="bold" fill="#606060" k="name" size="13" stroke="#ffffff"
                    stroke-width="2.0" />
            </m>
        </m>

        <m k="railway">
            <m k="tunnel" v="~|false|no">

                <m v="station">
                    <area fill="#9b9b79" />
                </m>

                <m k="bridge" v="yes|true">
                    <m v="tram">
                        <line cap="butt" fix="true" stroke="#cc000000" width="1.4" />
                    </m>
                    <m v="subway|light_rail|narrow_gauge">
                        <line cap="butt" fix="true" stroke="#cc000000" width="1.6" />
                    </m>
                    <m v="rail">
                        <line cap="butt" fix="true" stroke="#cc000000" width="1.6" />
                    </m>
                </m>

                <m v="tram">
                    <line cap="butt" stroke="#000000" width="0.25" stipple="9" />
                </m>
                <m v="subway">
                    <line cap="butt" stroke="#0f0f4c" width="0.4" />
                    <line cap="butt" dasharray="12,18" stroke="#b7b7e6" width="0.25" />
                </m>
                <m v="light_rail">
                    <line cap="butt" fade="12" stipple="10" stipple-stroke="#b7e6e6"
                        stipple-width="0.8" stroke="#0f4c0f" width="0.4" />
                </m>
                <m v="narrow_gauge">
                    <line cap="butt" stroke="#333333" width="0.4" />
                    <line cap="butt" stroke="#e6e6e6" width="0.25" />
                    <line cap="butt" dasharray="18,18" stroke="#333333" width="0.25" />
                </m>
                <m v="rail">
                    <line cap="butt" fade="12" fix="true" stipple="10" stipple-stroke="#e6e6e6"
                        stipple-width="0.8" stroke="#333333" width="1.6" />
                </m>
            </m>
        </m>


        <m k="boundary">
            <m k="boundary" v="administrative">
                <m k="admin_level">
                    <m v="11">
                        <line dasharray="1, 5" fix="true" stroke="#f9574b" width="1.1" />
                    </m>
                    <m v="10">
                        <line dasharray="5, 25" fix="true" stroke="#f9574b" width="1.1" />
                    </m>
                    <m v="9">
                        <line dasharray="15, 15" fix="true" stroke="#f9574b" width="1.1" />
                    </m>
                    <m v="8">
                        <line dasharray="15, 5, 5, 5" fix="true" stroke="#f9574b" width="1.1" />
                    </m>
                    <m v="7">
                        <line fix="true" stroke="#f9574b" width="1.1" />
                    </m>
                    <m v="6">
                        <line dasharray="5, 5" fix="true" stroke="#f9574b" width="1.25" />
                    </m>
                    <m v="5">
                        <line dasharray="15, 15" fix="true" stroke="#f9574b" width="1.25" />
                    </m>
                    <m v="4">
                        <line dasharray="15, 5, 5, 5" fix="true" stipple="4"
                            stipple-stroke="#f9574b" stipple-width="0.9" stroke="#80000000"
                            width="1.25" />
                    </m>
                    <m v="3">
                        <line fix="true" stroke="#f9574b" width="1.25" />
                    </m>
                    <m v="2">
                        <!-- <line stroke="#80ffff00" width="0.75" />
                          <line stroke="#f9574b" width="1.25" fix="true" dasharray="15, 15" /> -->
                        <line dasharray="15, 5, 5, 5" fix="true" stipple="6"
                            stipple-stroke="#80ffff00" stipple-width="0.9" stroke="#f9574b"
                            width="1.25" />
                    </m>
                </m>
            </m>
        </m>

        <m k="place" v="locality" zoom-min="17">
            <caption style="bold" fill="#000000" k="name" size="13" stroke="#ffffff"
                stroke-width="2.0" />
        </m>
    </m>


    <!--All POIs and WAYs with names-->
    <m k="name" select="first">

        <!--
        <m k="aeroway">
            <m v="aerodrome|airport" zoom-min="13">
                <symbol src="assets:aerodrom.svg" />
            </m>
            <m k="name" zoom-min="16">
                <text use="caption-small-blue" />
            </m>
        </m>
        -->

        <m k="amenity">
            <m select="first"> <!--Education-->
                <m v="kindergarten" zoom-min="17">
                    <symbol src="assets:vrtic.svg" />
                </m>
                <m v="library" zoom-min="17">
                    <symbol src="assets:biblioteka.svg" />
                </m>
                <m v="school" zoom-min="17">
                    <symbol src="assets:skola.svg" />
                </m>
                <m v="university|college" zoom-min="17">
                    <symbol src="assets:fakultet.svg" />
                </m>
                <m v="studio" zoom-min="17">
                    <symbol src="assets:dot_blue.svg" />
                </m>
                <m v="place_of_worship" zoom-min="17">
                    <symbol src="assets:dot_blue.svg" />
                </m>
                <m select="when-matched" zoom-min="18">
                    <text priority="-50" use="education" />
                </m>
            </m>

            <m select="first"> <!--Health-->
                <m v="hospital" zoom-min="15">
                    <symbol src="assets:bolnica.svg" />
                </m>
                <m select="when-matched" zoom-min="17">
                    <text priority="1" use="health" />
                </m>
            </m>
        </m>

        <m k="office" select="first">
            <m v="educational_institution" zoom-min="16">
            <symbol src="assets:dot_blue.svg" />
                <caption style="bold" fill="#000000" k="name" size="13" stroke="#ffffff"
                    stroke-width="2.0" />
            </m>
        </m>

        <m k="place" select="first">
            <m v="neighbourhood" zoom-min="13">
                <caption style="bold" fill="#000000" k="name" size="13" stroke="#ffffff"
                    stroke-width="2.0" />
            </m>
            <m v="suburb|town|village" zoom-min="10">
                <caption style="bold" fill="#000000" k="name" size="15" stroke="#ffffff"
                    stroke-width="2.0" />
            </m>
            <m v="island" zoom-min="10">
                <caption style="bold" fill="#000000" k="name" size="20" stroke="#ffffff"
                    stroke-width="2.0" />
            </m>
            <m v="city" zoom-max="14">
                <caption style="bold" fill="#000000" k="name" size="25" stroke="#ffffff"
                    stroke-width="2.0" />
            </m>
            <m v="country" zoom-max="6">
                <caption style="bold" fill="#000000" k="name" size="25" stroke="#ffffff"
                    stroke-width="2.0" />
            </m>
            <m v="hamlet" zoom-min="14">
                <caption style="bold" fill="#000000" k="name" size="12" stroke="#ffffff"
                    stroke-width="2.0" />
            </m>
        </m>

        <!-- railway -->
        <m k="railway">
            <m v="halt|station" zoom-min="12">
                <symbol src="assets:zeleznicka_stanica.svg" />
                <caption style="bold" dy="20" fill="#af3a3a" k="name" size="12" stroke="#ffffff"
                    stroke-width="2.0" />
            </m>

            <m v="tram_stop" zoom-min="16">
                <symbol src="assets:tramvajska_stanica.svg" />
                <caption style="bold" dy="20" fill="#af3a3a" k="name" size="11" stroke="#ffffff"
                    stroke-width="2.0" />
            </m>
        </m>

        <!--Match all missed buildings-->
        <m k="building|building:part" zoom-min="17">
            <text priority="2" use="caption-small-blue" />
            <symbol src="assets:dot_blue.svg" />
        </m>
    </m>

        <m k="highway">
            <m v="bus_stop" zoom-min="15">
                <symbol src="assets:autobuska_stanica.svg" />
                <caption style="bold" dy="16" fill="#000099" priority="100" k="name" size="11" stroke="#ffffff"
                    stroke-width="2.0" />
            </m>
            <m v="traffic_signals" zoom-min="17">
                <symbol src="assets:semafor.svg" />
            </m>
        </m>

</rendertheme>
